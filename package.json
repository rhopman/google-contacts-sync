{
  "name": "google-contacts-sync",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky",
    "generate:favicon": "svg-to-ico public/favicon.svg public/favicon.ico",
    "generate:env": "set -a; [ -f ./.env ] && . ./.env; set +a; envsubst < src/app/environment.template.ts | tee src/app/environment.ts",
    "prebuild": "npm run generate:env && npm run generate:favicon",
    "build": "ng build",
    "start": "ng serve",
    "test": "jest --config jest.config.ts --runInBand",
    "test:watch": "jest --config jest.config.ts --watch",
    "lint": "ng lint && prettier . --check && stylelint 'src/**/*.scss'",
    "prettier": "prettier . --check",
    "format": "prettier . --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.3",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/material": "^20.2.7",
    "@angular/platform-browser": "^20.3.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.4",
    "@angular/cli": "^20.3.4",
    "@angular/compiler-cli": "^20.3.3",
    "@types/jest": "^30.0.0",
    "angular-eslint": "20.3.0",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "stylelint": "^16.21.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "svg-to-ico": "^1.0.17",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.36.0",
    "zone.js": "^0.15.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache",
    "*.{css,scss}": "stylelint",
    "*": "prettier --check --ignore-unknown"
  }
}
